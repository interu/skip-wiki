- if SkipEmbedded::OpFixation.sso_enabled? && skip_url = SkipEmbedded::InitialSettings["skip_collaboration"]["skip_url"]
  %link{:rel => "shortcut icon", :href => URI.join(skip_url, "custom/favicon.ico")}
- else
  %link{:rel => "shortcut icon", :href => url_for("/favicon.ico")}
:javascript
  var relative_url_root = '#{ActionController::Base.relative_url_root}';
%meta{"http-equiv"=>"Content-Type", :content=>"text/html; charset=utf-8"}
%meta{"http-equiv"=>"Content-Script-Type", :content=>"text/javascript"}

%title&= [@title, "Wiki"].compact.join(" - ")

= stylesheet_link_tag 'jquery/cupertino/jquery-ui-1.7.1.custom.css'
= stylesheet_link_tag "application"
= stylesheet_link_tag 'skip/style'
= stylesheet_link_tag 'style'
= stylesheet_link_tag 'skip.rich_style'
= stylesheet_link_tag 'round-button'
= stylesheet_link_tag :skip_embedded_sprites

= javascript_include_tag "jquery-1.3.2.min", "jquery-ui-1.7.custom.min", "jrails"
= javascript_include_tag :skip_embedded
= javascript_include_file 'application'
= yield :header

:javascript
  jQuery(function(){
    jQuery("input[type=submit]").addClass("input_submit");
    jQuery("input[type=text]").addClass("input_text");
  });
