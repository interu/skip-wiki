%tr.user_name
  %th= f.label :name, _("User|Name")
  %td= SkipEmbedded::OpFixation.sso_enabled? ? @user.name : f.text_field(:name)
%tr.user_display_name
  %th= f.label :display_name, _("User|Display name")
  %td= SkipEmbedded::OpFixation.sso_enabled? ? @user.display_name : f.text_field(:display_name)

